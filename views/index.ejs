				</div>

				<div class="menu_copyright menu_mm">Shubham All rights reserved</div>
			</div>

		</div>

	</div>

<!-- Home -->
<div class="home">
  <div class="home_background" style="background-image:url(images/home.jpg)"></div>
  <div class="home_content">
    <div class="home_content_inner">
      <div class="home_text_large">Breathe</div>
      <div class="home_text_small">FRESHNESS ALL THE WAY</div>
    </div>
  </div>
</div>

<!-- Find Form -->

<div class="find">
  <div class="find_background parallax-window" data-parallax="scroll" data-image-src="images/find.jpg" data-speed="0.8"></div>
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="find_title text-center">Find the safest route</div>
      </div>
      <div class="col-12">
        <div class="find_form_container">
          <form action="/addpoints" method="POST" id="find_form" class="find_form d-flex flex-md-row flex-column align-items-md-center align-items-start justify-content-md-between justify-content-start flex-wrap">
            <div class="find_item">
              <div>Source :</div>
              <input type="text" class="source find_input" id="start" name="sr" value="<%=from%>" required="required" placeholder="City name">
            </div>
            <div class="find_item">
              <div>Destination :</div>
              <input type="text" class="destination find_input" required="required" id="end" name="dn" placeholder="City Name" value="<%=to%>">
            </div>
            <div class="find_item">
              <div>Pollutant :</div>
              <select name="p" id="mode" class="dropdown_item_select find_input" required>
								<option value="">&nbsp;</option>
			          <option value="pm2.5">PM<span id="subscript">2.5</span></option>
			          <option value="pm10">PM<span id="subscript">10</span></option>
			          <option value="no2">NO<span id="subscript">2</span></option>
			          <option value="so2">SO<span id="subscript">2</span></option>
			          <option value="co">CO</option>
			          <option value="o3">O<span id="subscript">3</span></option>
              </select>
            </div>
            <button class="button find_button " type="submit" >Find</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

